R2_ENDPOINT_URL="{{ r2.endpoint_url }}"
R2_ACCESS_KEY_ID="{{ r2.access_key_id }}"
R2_SECRET_ACCESS_KEY="{{ r2.secret_access_key }}"
R2_BUCKET_NAME="{{ r2.bucket_name }}"

FOREST_HOST="forest-{{ network }}-{{ forest.node_type }}"
FOREST_TOKEN_PATH="{{ forest.container.data_path }}/token"
CHAIN="{{ network }}"
SNAPSHOT_FORMAT="{{ forest.snapshots_format }}"
SNAPSHOT_PATH="{{ forest.container.snapshot_path }}"
BUILD_LATEST_SNAPSHOTS="{{ forest.build_snapshots.latest }}"

GRAFANA_ALLOY_API_KEY="{{ grafana_alloy.api_key }}"

RABBITMQ_HOST="rabbitmq"
RABBITMQ_USER="{{ rabbitmq.user }}"
RABBITMQ_PASS="{{ rabbitmq.password }}"

SLACK_TOKEN="{{ slack.token }}"
SLACK_CHANNEL="{{ slack.channel }}"

METRICS_PORT="{{ forest.metrics.port }}"
LOG_LEVEL="{{ forest.log_level }}"
